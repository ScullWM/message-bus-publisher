{
    "name": "wakeonweb/message-bus-publisher",
    "type": "library",
    "description": "WakeOnWeb MessageBus publisher",
    "keywords": [
        "message-bus"
    ],
    "homepage": "https://bitbucket.org/wakeonweb/message-bus-publisher",
    "license": "private",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0",
        "prooph/service-bus": "^6.0"
    },
    "suggest": {
        "bernard/bernard": "Publish asynchronously &| publish in amqp queue.",
        "guzzlehttp/guzzle": "Communicate threw HTTP gateway."
    },
    "autoload": {
        "psr-4": {
            "WakeOnWeb\\MessageBusPublisher\\": "src"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Tests\\WakeOnWeb\\SalesforceClient\\":"tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
